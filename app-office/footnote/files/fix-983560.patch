=== added file 'org.pantheon.footnote.gschema.xml'
--- org.pantheon.footnote.gschema.xml	1970-01-01 00:00:00 +0000
+++ org.pantheon.footnote.gschema.xml	2012-05-18 15:00:30 +0000
@@ -0,0 +1,34 @@
+<schemalist>
+	<enum id="window-states">
+		<value nick="Normal" value="0" />
+		<value nick="Maximized" value="1" />
+		<value nick="Fullscreen" value="2" />
+	</enum>
+	<schema path="/org/pantheon/footnote/" id="org.pantheon.footnote" gettext-domain="footnote">
+		<key name="window-width" type="i">
+			<default>640</default>
+			<summary>The saved width of the window.</summary>
+			<description>The saved width of the window.</description>
+		</key>
+		<key name="window-height" type="i">
+			<default>480</default>
+			<summary>The saved height of the window.</summary>
+			<description>The saved height of the window.</description>
+		</key>
+		<key name="sidebar-width" type="i">
+			<default>200</default>
+			<summary>The saved width of the sidebar.</summary>
+			<description>The saved width of the sidebar.</description>
+		</key>
+		<key name="window-state" enum="window-states">
+			<default>"Normal"</default>
+			<summary>The saved state of the window.</summary>
+			<description>The saved state of the window.</description>
+		</key>
+		<key name="view-mode" type="i">
+		    <default>0</default>
+		    <summary>The last used view</summary>
+		    <description>The view that was used when footnote was closed for next startup, 0 for note view, 1 for miller view</description>
+		</key>
+	</schema>
+</schemalist>

